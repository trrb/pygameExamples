a = input().split()
for i in range(len(a)):
    a[i] = int(a[i])
b = min(a)
ll = list()
for i in range(len(a)):
    if int(b) % 2 == 0:
        if int(a[i]) % 2 == 0:
            ll.append(int(a[i]))
    else:
        if int(a[i]) % 2 != 0:
            ll.append(int(a[i]))
p = sorted(ll)
for i in range(len(p)):
    p[i] = str(p[i])
print(" ".join(p))