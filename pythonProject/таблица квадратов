num_words = int(input())
words = []
for i in range(num_words):
    word = input().lower()
    words.append(word)
words.sort()


def anagram(a, b):
    return sorted(list(a)) == sorted(list(b))


while len(words) > 0:
    letter = [words.pop(0)]
    count = 0
    for i in range(len(words)):
        wrd = words[count]
        if anagram(letter[0], wrd):
            letter.append(wrd)
            words.remove(wrd)
        else:
            count += 1
    if len(letter) > 1:
        print(*letter)